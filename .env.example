# 服务器配置
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# WebSocket 配置
WS_MAX_CONNECTIONS=10
WS_PING_INTERVAL=20
WS_PING_TIMEOUT=60

# iFlow 配置
# 默认工作目录（留空则使用当前目录）
# IFLOW_DEFAULT_WORKING_DIR=
IFLOW_APPROVAL_MODE=YOLO

# 模型配置
IFLOW_DEFAULT_MODEL=glm-4.7

# 日志配置
LOG_LEVEL=INFO

# 前端配置
TERMINAL_THEME=dark
TERMINAL_FONT_FAMILY=Consolas, Monaco, 'Courier New', monospace
TERMINAL_FONT_SIZE=14px